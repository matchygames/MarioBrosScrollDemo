void init_tft() {
	if (TFT_VCC > -1) {
		pinMode(TFT_VCC, OUTPUT);
		digitalWrite(TFT_GND, HIGH);
	}
	if (TFT_GND > -1) {
		pinMode(TFT_GND, OUTPUT);
		digitalWrite(TFT_GND, LOW);
	}
	if (TFT_LED > -1) {
		pinMode(TFT_LED, OUTPUT);
		if (TFT_LED > 12) {
			digitalWrite(TFT_LED, HIGH);
		} else {
			analogWrite(TFT_LED, 255);
		}
	}
	if (SND_LEFT > -1) {
		pinMode(SND_LEFT, OUTPUT);
	}
	tft.initR(INITR_BLACKTAB);
	tft.setRotation(3);
	tft.setTextWrap(false);
	tft.setTextSize(1);
}

void init_map() {
	tile.width =  pgm_read_byte(sprite_tile_1 + 0);
	tile.height = pgm_read_byte(sprite_tile_1 + 1);
	grid.width = tft.width() / tile.width;
	grid.height = tft.height() / tile.height;
	level.width = pgm_read_byte(map_level_b + 0);
	level.height = pgm_read_byte(map_level_b + 1);
	pos.x = level.width - grid.width + 1;
	frame.x = 1;
}

