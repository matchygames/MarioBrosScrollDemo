				x0 = x - 1 + pos.x;
				y0 = y + pos.y;
				i = 2 + x0 + (y0 * level.width);
				tile_id2 = pgm_read_byte(temp_map + i);
				if (frame.x == 0) {
					temp_sprite = sprite_tile(tile_id);
				} else {
					temp_sprite = sprite_tile_b(tile_id);
				}
				if (tile_id > 1 || tile_id2 > 1) {
					draw_sprite(temp_sprite, x * tile.width, y * tile.height);
				}
			}
		}
	}
}

void setup() {
	init_tft();
	init_map();
}

void calc_pos() {
	pos.x++;
	if (pos.x > level.width - grid.width) {
		pos.x = 0;
		frame.x++;
		if (frame.x > 1) {
			frame.x = 0;
			level.width = pgm_read_byte(map_level + 0);
			level.height = pgm_read_byte(map_level + 1);
			tft.fillScreen(0x6c5f);
		} else {
			level.width = pgm_read_byte(map_level_b + 0);
			level.height = pgm_read_byte(map_level_b + 1);
			tft.fillScreen(0x0000);
		}
	}
}

void loop() {
	calc_pos();
	draw_map();
}
